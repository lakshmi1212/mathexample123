{
  "agent_role": "ci_workflow_16generator",
  "repository": {
    "url": "https://github.com/lakshmi1212/mathexample123",
    "default_branch": "main",
    "feature_branch": "Feature3"
  },
  "workflow": {
    "file_path": ".github/workflows/ci.yml",
    "name": "CI Pipeline"
  },
  "triggers": {
    "push": ["Feature3"],
    "pull_request": ["main"]
  },
  "runtime": {
    "language": "python",
    "version": "3.10"
  },
  "dependencies": [
    "pytest==8.0.0",
    "pytest-html==4.1.1"
  ],
  "test_execution": {
    "test_directory": "tests",
    "pytest_command": "python -m pytest tests/ -v --tb=short --junitxml=reports/report.xml --html=reports/report.html --self-contained-html",
    "fail_on_error": true
  },
  "test_reports": {
    "enabled": true,
    "report_directory": "reports",
    "formats": [
      "junit",
      "html"
    ],
    "junit_file": "reports/report.xml",
    "html_file": "reports/report.html"
  },
  "s3_upload": {
    "enabled": false,
    "bucket_name": "",
    "prefix": "",
    "region": "",
    "versioning_strategy": ""
  },
  "constraints": {
    "modify_source_files": false,
    "modify_test_files": false
  },
  "commit": {
    "enabled": true,
    "message": "Add CI workflow for Python tests"
  }
}
